{%- endmacro -%}

{%- macro dssVersion() -%}
  dss_version   
{%- endmacro -%}

{%- macro dssCurrentFlag() -%}
  dss_current_flag    
{%- endmacro -%}

{%- macro dssChangeHash() -%}
  dss_change_hash   
{%- endmacro -%}

{%- macro dssLoadDate() -%}
  dss_load_datetime
{%- endmacro -%}

{%- macro dssRecordSource() -%}
  dss_record_source
{%- endmacro -%}

{# --==============================================================================                                                                         -- #}
{# -- Custom Columns                                                                                                                                        -- #}
{# --==============================================================================                                                                         -- #}

{%- macro dssSequenceNo() -%}
  dss_sequence_no
{%- endmacro -%}  

{%- macro dssEventDate() -%}
  dss_event_datetime
{%- endmacro -%}

{%- macro dssEffectivityDate() -%}
  dss_effectivity_datetime
{%- endmacro -%}

{%- macro dssExpiryDate() -%}
