{%- endmacro -%}

{%- macro dssRowNo() -%}
  dss_row_no
{%- endmacro -%}

{%- macro isActive() -%}
  is_active
{%- endmacro -%}

{%- macro isFull() -%}
  is_full
{%- endmacro -%}

{%- macro SnapshotDate() -%}
  snapshot_datetime
{%- endmacro -%}

{# --==============================================================================                                                                         -- #}
{# -- Data Type                                                                                                                                             -- #}
{# --==============================================================================                                                                         -- #}

{%- macro dssDateTimeDataType() -%}
  DATETIME2(7)
{%- endmacro -%}

{%- macro hashDataType() -%}
  BINARY(16)
{%- endmacro -%}

{# --==============================================================================                                                                         -- #}
{# -- Values                                                                                                                                                -- #}
{# --==============================================================================                                                                         -- #}

{%- macro dssDateTimeMinValue() -%}
  '0001-01-01'
{%- endmacro -%}

{%- macro dssDateTimeMaxValue() -%}
  '9999-12-31'
{%- endmacro -%}

{%- macro dssChangeHashDefaultValue() -%}
  CONVERT(BINARY(16),'')
{%- endmacro -%}

{# --==============================================================================                                                                         -- #}
{# -- Formulas                                                                                                                                              -- #}
{# --==============================================================================                                                                         -- #}

{%- macro dssEndDateFormula() -%}
  DATEADD(NS,-100,{{ getSourceTableDssLoadDate() }})
{%- endmacro -%}

{%- macro currentDateTimeFormula() -%}
  SYSDATETIME()
{%- endmacro -%}

{# --==============================================================================                                                                         -- #}
