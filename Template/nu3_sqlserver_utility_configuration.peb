{# --==============================================================================                                                                         -- #}
{# -- Data Type                                                                                                                                             -- #}
{# --==============================================================================                                                                         -- #}

{%- macro dssDateTimeDataType() -%}
  DATETIME2(7)
{%- endmacro -%}

{# --==============================================================================                                                                         -- #}
{# -- Values                                                                                                                                                -- #}
{# --==============================================================================                                                                         -- #}

{%- macro dssDateTimeMinValue() -%}
  '0001-01-01'
{%- endmacro -%}

{%- macro dssDateTimeMaxValue() -%}
  '9999-12-31'
{%- endmacro -%}

{%- macro dssChangeHashDefaultValue() -%}
  CONVERT(BINARY(16),'')
{%- endmacro -%}

{# --==============================================================================                                                                         -- #}
{# -- Formulas                                                                                                                                              -- #}
{# --==============================================================================                                                                         -- #}

{%- macro dssEndDateFormula() -%}
  DATEADD(NS,-100,{{ getSourceTableDssLoadDate() }})
{%- endmacro -%}

{%- macro currentDateTimeFormula() -%}
  SYSDATETIME()
{%- endmacro -%}

