{# --==============================================================================                                                                         -- #}
{# -- FileGroup                                                                                                                                             -- #}
{# --==============================================================================                                                                         -- #}

{% macro getTableFileGroup(table = table) %}
  {%- if table.objectType == Types.ObjectType.Stage -%}
    STAGE
  {%- else -%}
    DATA
  {%- endif -%} 
{% endmacro %}

{%- macro getIndexFileGroup() -%}
  INDEXES
{%- endmacro -%}

